page{
  background: #f6f6f6;
  position: relative;
  z-index: 1;
}
.header{
  width: 100%;
  height: 230rpx;
  background:rgba(255,255,255,1);
  display: flex;
  padding:30rpx;
  box-sizing: border-box;
  border-top: 1rpx solid #f6f6f6;
  align-items: center;
}
.header .xiang-q{
  margin-left: 31rpx;
}
.header .xiang-q .title{
margin-top: 10rpx;
font-size:32rpx;
font-family:PingFangd;
font-weight:bold;
color:rgba(59,64,78,1);
}
.header .xiang-q .time{
margin-top: 18rpx;
font-size:24rpx;
font-family:PingFang;
color:rgba(165,170,184,1);
}
.header .xiang-q .price{
margin-top: 20rpx;
font-size:24rpx;
font-family:PingFang;
color:rgba(254,98,13,1);
}
.header .xiang-q .price .holiday{
 color:rgba(165,170,184,1);
}
.tiao{
  background: #f6f6f6;
  width: 100%;
  height: 20rpx;
}
.material{
  margin-top: 20rpx;
  background: #ffffff;
}
.material .title{
  width: 100%;
  height: 99rpx;
  border-bottom: 1rpx solid #f6f6f6;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.material .title .z-s{
width:6rpx;
height:30rpx;
background:rgba(84,137,253,1);
border-radius:3rpx;
}
.material .title .m-s{
font-size:30rpx;
font-family:PingFang;
color:rgba(59,64,78,1);
margin-left: 15rpx;
}

.material .title .add-m-s{
font-size:24rpx;
font-family:PingFang;
color:#A5AAB8;
margin-left: 12rpx;
}
.list{
  background: #ffffff;
}
.list .kind{
  width: 100%;
  height: 99rpx;
  padding-right: 30rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #f6f6f6;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.list .kind .kind-name{
width: 50%;
font-size:30rpx;
font-family:PingFang;
color:rgba(59,64,78,1);
}
.list .kind .date-name .picker{
  font-size:28rpx;
  font-family:PingFang; 
  color:rgba(59,64,78,1);
  float: left;
 
}
.list .kind .date-name{
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.picker-date{
  float: left;
  line-height: 99rpx;
}
.picker-time{
  float: right;
  line-height: 99rpx;
}
.b-zhu{
  width: 100%;
  height: 170rpx;
  padding: 35rpx 30rpx 0;
  box-sizing: border-box;
  background: #ffffff;
  margin-bottom: 138rpx;
  margin-top: 20rpx;
  z-index: 10;
}
.b-zhu .title{
font-size:30rpx;
font-family:PingFang;
color:rgba(59,64,78,1);
z-index: 9ï¼›
}
.b-zhu .m-c{
  display: block;
margin-top: 10rpx;
}
.button{
  z-index: 999;
  position:fixed;
  bottom: 0;
  width: 100%;
  height: 98rpx;
  text-align: center;
  line-height: 98rpx;
  font-size:34rpx;
  font-family:PingFang;
  color:rgba(255,255,255,1);
  background:#5078FC;
  opacity:0.5;
  border-radius:0px;
}
.active{
  background: #5078FC;
  opacity: 1
}
.buuton-t{
  z-index: 99;
  position:fixed;
  bottom: 0;
  width: 100%;
  height: 98rpx;
  background:rgba(255,255,255,1);
}

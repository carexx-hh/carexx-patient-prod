.mod_aside {
  position: fixed;
  bottom: 180rpx;
  right: -520rpx;
  transform: translateX(0);
  transition: transform 0.4s ease;
}

.mod_aside.open {
  transform: translateX(-520rpx);
}

.aside_nav {
  width: 520rpx;
  padding: 20rpx 0 20rpx;
  background: #fff;
  border-radius: 8rpx 0 0 8rpx;
  height: 184rpx;
  position: relative;
  overflow: hidden;
  z-index: 99;
  box-shadow: 0 0 10rpx rgba(0, 0, 0, 0.1);
}

.aside_nav .aside_nav_item {
  width: 130rpx;

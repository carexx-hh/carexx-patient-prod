/* components/down_loading/down_loading.wxss */
.down_loading{
  padding: 30rpx 0;
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;
  display: none;
  background: url(data:image/png;base64,) no-repeat center center/40rpx auto;
}
.down_loading.active{
  display: block;
  animation: rotate .8s ease;
}
@keyframes rotate{
  0%{
    transform:rotateZ(0deg);
  }
  100%{
    transform: rotateZ(360deg);
  }
}